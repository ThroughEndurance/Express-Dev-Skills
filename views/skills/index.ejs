<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kiefer's Skills</title>
</head>
 <body>
  <h1>Kiefers Skills as of <%= time %></h1>
   <ul>
     <% skills.forEach(function(skill) { %>
       <li>
        <a href="/skills/<%= skill.id %>"><%= skill.skill %></a>
         <%= skill.skill %>
           - 
         <%= skill.done ? 'done' : 'not done' %>
       </li>
     <% }); %>
   </ul>
   <a href="/skills/new">Add Skill</a>
   <a href="/">Homepage for Kiefers Skills</a>
 </body>
</html>